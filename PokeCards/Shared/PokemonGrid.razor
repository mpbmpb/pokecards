@using PokeCards.Data

<div class="pokemon-grid-container"> 
    <div class="pokemon-list">
        @foreach (var pokemon in Pokemons ?? new List<Pokemon>())
        {
            <SpeciesCard Pokemon="pokemon"></SpeciesCard>
        }
    </div>
</div>

@code {
    [Parameter]
    public IEnumerable<Pokemon>? Pokemons { get; set; }
    
}